{
  "name": "@lodgify/websites-services-client",
  "version": "0.0.1",
  "description": "A client for making it easy to interact with the Lodgify websites services 2.0",
  "main": "lib/index.js",
  "config": {
    "jest_config": "tools/jest/jest.config.js",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "./tools/semantic-release/release.config.js"
  },
  "scripts": {
    "build:js": "BABEL_ENV=production babel src/ -d lib/",
    "build": "npm run clean && npm run build:js",
    "clean": "rm -rf ./lib",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run build",
    "test": "jest --coverage --config $npm_package_config_jest_config",
    "test:watch": "jest --config $npm_package_config_jest_config --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lodgify/websites-services-client.git"
  },
  "keywords": [
    "services"
  ],
  "author": "Lodgify",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.0.0",
    "@lodgify/eslint-config": "^2.2.0",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsdoc": "^4.7.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.0",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3"
  },
  "dependencies": {
    "@lodgify/fetch-related-utils": "^1.0.4",
    "@lodgify/get-query-string": "^1.1.0"
  }
}
